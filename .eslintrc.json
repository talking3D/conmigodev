{
    "extends": "next/core-web-vitals",
    "plugins": ["unused-imports"],
    "rules": {
        // Additional, per-project rules...
        "no-console": "warn",
        "no-unused-vars": "off",
        "unused-imports/no-unused-imports": "warn",
        "unused-imports/no-unused-vars": [
            "warn",
            {
                "vars": "all",
                "varsIgnorePattern": "^_",
                "args": "after-used",
                "argsIgnorePattern": "^_"
            }
        ]
    }
}
